{
    "name": "Wiener Netze Smartmeter Sync Dev",
    "version": "0.1.0",
    "slug": "wnsm_sync_dev",
    "description": "Integration for Wiener Netze Smart Meter that fetches 15-min consumption data and pushes to Home Assistant statistics.",
    "url": "https://github.com/ZakiZtraki/haos-addon-wnsm-dev",
    "arch": ["amd64", "armv7", "aarch64"],
    "startup": "application",
    "boot": "auto",
    "map": [],
    "hassio_api": true,
    "homeassistant_api": true,
    "timeout": "300",
    "panel_icon": "mdi:flash",
    "codenotary": "admins@zenmedia.live",
    "options": {
        "WNSM_USERNAME": "",
        "WNSM_PASSWORD": "",
        "HA_URL": "",
        "STAT_ID": "sensor.wiener_netze_energy",
        "MQTT_USERNAME": "",
        "MQTT_PASSWORD": "",
        "MQTT_HOST": "core-mosquitto",
        "MQTT_PORT": 1883,
        "HISTORY_DAYS": "",
        "MQTT_TOPIC": "",
        "RETRY_COUNT": "",
        "RETRY_DELAY": "",
        "UPDATE_INTERVAL": ""
    },
    "schema": {
        "WNSM_USERNAME": "str",
        "WNSM_PASSWORD": "password",
        "HA_URL": "str",
        "STAT_ID": "str",
        "MQTT_USERNAME": "str",
        "MQTT_PASSWORD": "str",
        "MQTT_HOST": "str",
        "MQTT_PORT": "port",
        "HISTORY_DAYS": "str",
        "MQTT_TOPIC": "str",
        "RETRY_COUNT": "str",
        "RETRY_DELAY": "str",
        "UPDATE_INTERVAL": "int"
    },
    "build": true,
    "udev": true,
    "services": ["mqtt:need"]
}
